FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include	"constants"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $omega;

boundaryField
{
    inlet
    {
        type                freestream;
        freestreamValue     $internalField;
    }

    outlet
    {
        type                freestream;
	freestreamValue	    $internalField;
    }

    frontAndBack
    {
	type		    empty;
    }

    naca
    {
        type                omegaWallFunction;
        value		    uniform 1e3;
    }
}

